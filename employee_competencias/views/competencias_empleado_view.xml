<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="navintha_hr_form_view" model="ir.ui.view">
            <field name="name">hr.employee_navintha_competencias_view.inherited</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"></field>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                     <page string="Competencias">
                        <field name="x_navintha_comempleado" >
                            <tree>
                                <field name="name"></field>
                                <field name="x_navintha_nivelcompetencia"></field>
                                <field name="x_navintha_descripcion"></field>
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
  </data>
</openerp>

